{
  "name": "code-split-component",
  "version": "2.0.0-alpha.0",
  "description": "Declarative code splitting for your Wepback 2 bundled React projects, with SSR support.",
  "main": "commonjs/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf ./commonjs",
    "test": "mocha --opts .mocha.opts \"test/**/*.test.js\"",
    "test:watch": "mocha --opts .mocha.opts \"test/**/*.test.js\" -w -R min",
    "lint": "eslint modules",
    "build": "cross-env BABEL_ENV=commonjs babel ./modules -d ./commonjs",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "files": [
    "commonjs",
    "babel.js",
    "webpack.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ctrlplusb/code-split-component.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "babel plugin",
    "code splitting"
  ],
  "author": "Sean Matheson <sean@ctrlplusb.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ctrlplusb/code-split-component/issues"
  },
  "homepage": "https://github.com/ctrlplusb/code-split-component#readme",
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "app-root-path": "^2.0.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.5.0",
    "flow-bin": "^0.34.0",
    "mocha": "^3.1.2",
    "react": "^15.3.2",
    "rimraf": "^2.5.4",
    "webpack": "^2.1.0-beta.25"
  },
  "peerDependencies": {
    "babel-template": "^6.16.0",
    "react": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "md5": "^2.2.1"
  }
}
